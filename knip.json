{
  "$schema": "https://unpkg.com/knip/schemas/knip-schema.json",
  "entry": [
    "app/**/*.{ts,tsx,js,jsx}",
    "components/**/*.{ts,tsx,js,jsx}",
    "hooks/**/*.{ts,tsx,js,jsx}",
    "lib/**/*.{ts,tsx,js,jsx}",
    "next.config.{js,ts,mjs}",
    "postcss.config.{js,ts,mjs}"
  ],
  "ignore": [
    "node_modules/**",
    ".next/**",
    "tawkit-9.61/**",
    "data/**",
    "audio/**",
    "images/**",
    "themes/**",
    "font/**",
    "slides/**",
    "languages/**"
  ],
  "ignoreDependencies": [
    "markdownlint",
    "tailwindcss",
    "tailwindcss-animate",
    "autoprefixer"
  ]
}
