{
  "$schema": "https://unpkg.com/knip/schemas/knip-schema.json",
  "entry": [
    "app/**/*.{ts,tsx,js,jsx}",
    "features/**/*.{ts,tsx,js,jsx}",
    "widgets/**/*.{ts,tsx,js,jsx}",
    "services/**/*.{ts,tsx,js,jsx}",
    "entities/**/*.{ts,tsx,js,jsx}",
    "shared/**/*.{ts,tsx,js,jsx}",
    "next.config.{js,ts,mjs}",
    "postcss.config.{js,ts,mjs}"
  ],
  "ignore": [
    "node_modules/**",
    ".next/**",
    "tawkit-9.61/**",
    "data/**",
    "audio/**",
    "images/**",
    "themes/**",
    "font/**",
    "slides/**",
    "languages/**",
    "components/**",
    "hooks/**",
    "lib/**"
  ],
  "ignoreDependencies": [
    "markdownlint",
    "tailwindcss",
    "tailwindcss-animate",
    "autoprefixer",
    "prettier"
  ]
}
