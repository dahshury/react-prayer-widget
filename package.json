{
  "name": "tawkit-next",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "build": "next build",
    "dev": "next dev --turbo",
    "lint": "next lint",
    "start": "next start",
    "prepare": "husky install",
    "biome:check": "biome check --no-errors-on-unmatched .",
    "biome:fix": "biome check --write --no-errors-on-unmatched .",
    "typecheck": "tsc --noEmit",
    "knip": "pnpm dlx knip",
    "prettier:md:check": "pnpm dlx prettier --check \"**/*.md\" --ignore-path .prettierignore",
    "prettier:md:write": "pnpm dlx prettier --write \"**/*.md\" --ignore-path .prettierignore",
    "mdlint:check": "markdownlint \"**/*.md\" -i .prettierignore",
    "mdlint:fix": "markdownlint --fix \"**/*.md\" -i .prettierignore"
  },
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./components": "./components/index.ts",
    "./hooks": "./hooks/index.ts",
    "./lib": "./lib/index.ts"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.6",
    "@vercel/analytics": "1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "color": "^5.0.0",
    "countries-and-timezones": "^3.8.0",
    "country-data-list": "^1.5.5",
    "country-region-data": "^3.1.0",
    "geist": "^1.5.1",
    "lucide-react": "^0.543.0",
    "motion": "^12.23.12",
    "next": "15.5.3",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-circle-flags": "^0.0.23",
    "react-day-picker": "9.9.0",
    "react-dom": "^19.1.1",
    "react-use-measure": "^2.1.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "ultracite": "5.6.4"
  }
}
